{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "sourceMap": true,
    "rootDir": ".",
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "moduleResolution": "Node",
    "noImplicitThis": false,
    "esModuleInterop": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
